        Console.Write("Please Enter the quantity of donuts : ")
        count = Console.ReadLine()

        If count <= 9 Then
            Console.WriteLine("Number od Donuts : " & count)
        ElseIf count >= 10 Then
            Console.WriteLine("Many")
        End If
